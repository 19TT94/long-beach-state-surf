{{liquid-outlet}}
<div class="club shadow-top">
    <div class="club-profiles">
        <h1 class="club-profiles-title center">The Club</h1>

        <div class="row center">
            {{#each model as |club|}}
                <div class="column small-12 medium-4 profile">
                    <div class="profile__image" style="background-image: url('{{club.profile}}')" {{action 'toggleModal'}}></div>
                    <h2 class="profile__name">{{club.name}}</h2>
                    <p class="profile__postion">{{club.position}}</p>
                </div>
            {{/each}}
        </div>

    </div>
    <div class="calendar">
        <h1 class="calendar-title">Event Schedule</h1>
        <div class="calendar-container">
            <iframe src="https://calendar.google.com/calendar/embed?showTitle=0&amp;showNav=0&amp;showCalendars=0&amp;showTz=0&amp;height=600&amp;wkst=1&amp;bgcolor=%23cccccc&amp;src=qqfcvc6edimhpofsakeq3fg4e0%40group.calendar.google.com&amp;color=%232F6309&amp;ctz=America%2FLos_Angeles" style="border-width:0" width="800" height="600" frameborder="0" scrolling="no"></iframe>
        </div>
    </div>
</div>
